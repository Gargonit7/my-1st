{
  "name": "vegawallet-e2e-test",
  "version": "1.0.0",
  "description": "End-to-end tests for Vegawallet",
  "private": true,
  "main": "index.js",
  "scripts": {
    "cypress:open:local": "CYPRESS_ENV=localhost env-cmd yarn run cypress:open",
    "clean": "rm -r ./test-wallets; mkdir ./test-wallets",
    "cypress:open": "env-cmd cypress open",
    "test:local": "CYPRESS_ENV=localhost env-cmd yarn run test",
    "test:prod": "CYPRESS_ENV=production env-cmd yarn run test",
    "test": "cypress run --env TAGS='@e2e-test' --spec 'cypress/e2e/*.cy.js'",
    "test:debug:local": "CYPRESS_ENV=localhost env-cmd yarn run test:debug",
    "test:debug:prod": "CYPRESS_ENV=production env-cmd yarn run test:debug",
    "test:debug": "env-cmd cypress run --headed --browser chrome --env TAGS='@e2e-test' --spec 'cypress/e2e/*.cy.js'",
    "createcustomconfig": "node cypress/support/create-mock-config.js"
  },
  "author": "",
  "dependencies": {
    "@ethersproject/abstract-signer": "5.7.0",
    "@ethersproject/experimental": "5.7.0",
    "@ethersproject/providers": "5.7.2",
    "@ethersproject/wallet": "5.7.0",
    "axios": "^1.3.2",
    "cypress": "^10.11.0",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "lodash": "^4.17.21"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@cypress/code-coverage": "^3.10.0",
    "babel-loader": "^9.1.2",
    "cypress-downloadfile": "^1.2.1",
    "deepmerge": "^4.2.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.33.0",
    "eslint-plugin-cypress": "^2.11.3",
    "webpack": "^5.75.0"
  }
}
